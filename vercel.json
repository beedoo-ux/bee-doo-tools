{
  "rewrites": [
    { "source": "/(.*)", "destination": "/$1" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Bee-Doo-Version", "value": "2.0" }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/efs-sync",
      "schedule": "0 5 * * *"
    },
    {
      "path": "/api/cron-sync",
      "schedule": "0 * * * *"
    }
  ]
}
